{
  "name": "team-trade-web",
  "description": "TT web interface made from Naresh Bhatia react-force template",
  "version": "0.0.1",
  "author": "Naresh Bhatia, Alexander Kireev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zishe/team-trade-web.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:prod": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "gql:schema": "apollo service:download -c apollo.config.js",
    "gql:types": "apollo client:codegen -c apollo.config.js --localSchemaFile=schema.json --target=typescript --globalTypesFile=src/graphql-types.ts",
    "format": "prettier --write README.md 'src/**/{*.md,*.json,*.ts*}'"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@nareshbhatia/react-force": "^1.19.2",
    "@types/history": "^4.7.3",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "graphql": "^14.5.8",
    "history": "^4.10.1",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "mobx-state-router": "^4.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-login": "^5.0.7"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/graphql": "^14.5.0",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.15",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "apollo": "^2.21.1",
    "apollo-codegen": "^0.20.2",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-scripts": "3.3.0",
    "typescript": "3.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

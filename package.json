{
  "name": "team-trade-web",
  "description": "TT web interface made from Naresh Bhatia react-force template",
  "version": "0.0.1",
  "author": "Naresh Bhatia, Alexander Kireev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zishe/team-trade-web.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "react-scripts start --watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:prod": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "gql:schema": "apollo service:download -c apollo.config.js",
    "gql:types": "apollo client:codegen -c apollo.config.js --localSchemaFile=schema.json --target=typescript --globalTypesFile=src/graphql-types.ts",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write README.md 'src/**/{*.md,*.json,*.ts*}'",
    "fix:lint": "eslint --fix --ext=jsx,ts,tsx src"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@material-ui/core": "4.8.1",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.37",
    "@nareshbhatia/react-force": "1.19.2",
    "@types/history": "4.7.3",
    "apollo-boost": "0.4.7",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "graphql": "14.5.8",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "micro-observables": "1.2.0",
    "mobx": "5.15.1",
    "mobx-react": "6.1.4",
    "mobx-state-router": "4.0.6",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-google-login": "5.0.7",
    "react-scripts": "3.3.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/graphql": "14.5.0",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.15",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0",
    "apollo": "2.21.2",
    "apollo-codegen": "0.20.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.8.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "typescript": "3.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
